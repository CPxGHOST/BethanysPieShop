@model  BethanysPieShop.ViewModels.PieListViewModel
<!DOCTYPE html>
<html>
<head>
    <title>Bethany's Pie Shop'</title>
</head>
<body>
    <h1>@Model.Category</h1>

    @foreach (var pie in Model.Pies)
    {
        <h2>@pie.Name</h2>
        <h2>@pie.Price.</h2>
        <h2>@pie.LongDescription.</h2>
    }

</body>
</html>